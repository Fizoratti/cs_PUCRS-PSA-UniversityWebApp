@using Microsoft.AspNetCore.Identity
@using Entidades.Models;
@model IEnumerable<Entidades.Models.ApplicationUser>



@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Relatório de alunos matriculados</h4>

@*<div class="col-md-2">
    <ul class="list-group">
        <li class="list-group-item">Disciplina selecionada: @ViewData["codcred"] </li>
    </ul>
</div>*@

<br />

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                <p>Matrícula</p>
            </th>
            <th>
                <p>Aluno</p>
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>

        </tr>
        }
    </tbody>
</table>
